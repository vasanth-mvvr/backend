pipeline{
    agent{
        label 'AGENT-1'
    }
    options{
        timeout(time: 30,unit: 'MINUTES')
        disableConcurrentBuilds()
        ansiColor('xterm')
    }
    stages{
        stage("Install"){
            steps{
                sh """
                This is test message
                    ls -ltr
                    npm install
                
                """
            }
        post{
            always {
                
                deleteDir()
            }
            success{
                echo 'success'
            }
            failure{
                echo 'failure'
            }
        }
    }
}